{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "downlevelIteration": true,
    "lib": ["es5", "es2015", "es2017", "dom", "dom.iterable", "scripthost"],
    "allowJs": true,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": false,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {},
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": "./dist"
  },
  "exclude": ["node_modules", "build", "dist", "test"]
}
